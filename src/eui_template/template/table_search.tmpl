{{define "table_search"}}
	<div class="table-search">
		<form>
			<div class="block-box table-search-box">
				<div class="col-xl-3 col-lg-3 col-md-4 col-sm-6">
					<div class="form-group">
						<div class="input-group">
							<span class="prepend">服务名称</span>
							<input name="name" type="text" placeholder="请输入服务名称" autocomplete="off">
						</div>
					</div>
				</div>
				<div class="col-xl-3 col-lg-3 col-md-4 col-sm-6">
					<div class="form-group">
						<div class="input-group">
							<span class="prepend">接口地址</span>
							<input name="api" type="text" placeholder="请输入接口地址" autocomplete="off">
						</div>
					</div>
				</div>
				<div class="col-xl-3 col-lg-3 col-md-4 col-sm-6">
					<div class="form-group">
						<div class="input-group">
							<span class="prepend">创建日期</span>
							<input id="table-datepicker-2" class="table-datepicker" name="addtime" data-key="2"
								   type="text" placeholder="请选择创建日期" autocomplete="off">
						</div>
					</div>
				</div>
				<div class="col-xl-3 col-lg-3 col-md-4 col-sm-6">
					<div class="form-group">
						<div class="input-group">
							<span class="prepend">有效日期</span>
							<input id="table-datepicker-3" class="table-datepicker" name="realdate" data-key="3"
								   type="text" placeholder="请选择有效日期" autocomplete="off">
						</div>
					</div>
				</div>
				<div class="col-xl-3 col-lg-3 col-md-4 col-sm-6">
					<div class="form-group"><select name="type" data-width="100%">
							<option value="">
								请选择请求方式
							</option>
							<option value="get">
								GET
							</option>
							<option value="post">
								POST
							</option>
						</select>
						<div style="width:100%;" class="select" data-open="0">
								<span class="">
												请选择请求方式
											</span>
							<i class="ri-arrow-down-s-line rotate-0"></i>
							<i class="ri-close-circle-fill select-clear"></i>
						</div>
						<div style="width:100%;" class="select-option animated faster">
							<ul class="iscroll ps">
								<li class="" data-val="get">
									GET
								</li>
								<li class="" data-val="post">
									POST
								</li>
								<div class="ps__rail-x" style="left: 0px; bottom: 4px;">
									<div class="ps__thumb-x" tabindex="0" style="left: 0px; width: 0px;"></div>
								</div>
								<div class="ps__rail-y" style="top: 0px; right: 4px;">
									<div class="ps__thumb-y" tabindex="0" style="top: 8px;"></div>
								</div>
							</ul>
						</div>
					</div>
				</div>
				<div class="col-xl-3 col-lg-3 col-md-4 col-sm-6">
					<div class="form-group"><select name="status" data-width="100%">
							<option value="">
								请选择状态
							</option>
							<option value="1">
								正常
							</option>
							<option value="0">
								禁用
							</option>
						</select>
						<div style="width:100%;" class="select" data-open="0">
								<span class="">
												请选择状态
											</span>
							<i class="ri-arrow-down-s-line rotate-0"></i>
							<i class="ri-close-circle-fill select-clear"></i>
						</div>
						<div style="width:100%;" class="select-option animated faster">
							<ul class="iscroll ps">
								<li class="" data-val="1">
									正常
								</li>
								<li class="" data-val="0">
									禁用
								</li>
								<div class="ps__rail-x" style="left: 0px; bottom: 4px;">
									<div class="ps__thumb-x" tabindex="0" style="left: 0px; width: 0px;"></div>
								</div>
								<div class="ps__rail-y" style="top: 0px; right: 4px;">
									<div class="ps__thumb-y" tabindex="0" style="top: 8px;"></div>
								</div>
							</ul>
						</div>
					</div>
				</div>
				<div class="col-xl-3 col-lg-3 col-md-4 col-sm-6">
					<button class="search-do hl middle">
						<i class="ri-search-line"></i>搜索
					</button>
					<button class="search-refresh middle">
						<i class="ri-refresh-line"></i>重置
					</button>
				</div>
			</div>
		</form>
	</div>
{{end}}